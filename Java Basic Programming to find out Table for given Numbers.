package com.company;
import java.util.Scanner;
public class ToFindTableOfGivenNumber {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the Number to get table of it for ten times:");
        int a = sc.nextInt();
        for (int i = 1; i<=10; i++){
            System.out.println(a*i);

        }

    }
}
